<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Alexander Anishchik</Authors>
    <Company>Alexander Anishchik</Company>
    <PackageProjectUrl>https://github.com/AlexanderAnishchik/Credit-Card-Stripe-Parser</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/AlexanderAnishchik/Credit-Card-Stripe-Parser/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/AlexanderAnishchik/Credit-Card-Stripe-Parser</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Library for parsing magnetic stripe track data of credit cards. Validate LRC and parse Track 1 and Track 2 into models.</Description>
    <Product>Credit Card Stripe Parser</Product>
    <Version>1.1.0</Version>
  </PropertyGroup>

</Project>
